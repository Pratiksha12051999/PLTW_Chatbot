# Frontend Environment Variables
# Copy this file to .env.local and fill in your values

# WebSocket API URL (from CDK WebSocketStack output)
NEXT_PUBLIC_WEBSOCKET_URL=wss://your-api-id.execute-api.us-east-1.amazonaws.com/prod

# REST API URL (from CDK RestApiStack output)
NEXT_PUBLIC_REST_API_URL=https://your-api-id.execute-api.us-east-1.amazonaws.com/prod
